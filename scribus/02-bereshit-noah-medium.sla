            <StoryText>
                <DefaultStyle LINESPMode="2">
                    <Tabs Type="1" Pos="143.346456692913" Fill=""/>
                </DefaultStyle>
                <ITEXT CPARENT="verse no" CH="ט"/>
                <ITEXT CPARENT="regular" CH=" אֵ֚לֶּה תּוֹלְדֹ֣ת נֹ֔חַ נֹ֗חַ אִ֥ישׁ צַדִּ֛יק תָּמִ֥ים הָיָ֖ה בְּדֹֽרֹתָ֑יו אֶת־הָֽאֱלֹהִ֖ים הִֽתְהַלֶּךְ־נֹֽחַ׃ "/>
                <ITEXT CPARENT="verse no" CH="י"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וַיּ֥וֹלֶד נֹ֖חַ שְׁלֹשָׁ֣ה בָנִ֑ים אֶת־שֵׁ֖ם אֶת־חָ֥ם וְאֶת־יָֽפֶת׃ "/>
                <ITEXT CPARENT="verse no" CH="יא"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וַתִּשָּׁחֵ֥ת הָאָ֖רֶץ לִפְנֵ֣י הָֽאֱלֹהִ֑ים וַתִּמָּלֵ֥א הָאָ֖רֶץ חָמָֽס׃ "/>
                <ITEXT CPARENT="verse no" CH="יב"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וַיַּ֧רְא אֱלֹהִ֛ים אֶת־הָאָ֖רֶץ וְהִנֵּ֣ה נִשְׁחָ֑תָה כִּֽי־הִשְׁחִ֧ית כָּל־בָּשָׂ֛ר אֶת־דַּרְכּ֖וֹ עַל־הָאָֽרֶץ׃"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="portion" CH="ס"/>
                <para LINESPMode="2" LINESP="20" MinWordTrack="0.9"/>
                <ITEXT CPARENT="verse no" CH="יג"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וַיֹּ֨אמֶר אֱלֹהִ֜ים לְנֹ֗חַ קֵ֤ץ כָּל־בָּשָׂר֙ בָּ֣א לְפָנַ֔י כִּֽי־מָלְאָ֥ה הָאָ֛רֶץ חָמָ֖ס מִפְּנֵיהֶ֑ם וְהִנְנִ֥י מַשְׁחִיתָ֖ם אֶת־הָאָֽרֶץ׃ "/>
                <ITEXT CPARENT="verse no" CH="יד"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="עֲשֵׂ֤ה לְךָ֙ תֵּבַ֣ת עֲצֵי־גֹ֔פֶר קִנִּ֖ים תַּֽעֲשֶׂ֣ה אֶת־הַתֵּבָ֑ה וְכָֽפַרְתָּ֥ אֹתָ֛הּ מִבַּ֥יִת וּמִח֖וּץ בַּכֹּֽפֶר׃ "/>
                <ITEXT CPARENT="verse no" CH="טו"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וְזֶ֕ה אֲשֶׁ֥ר תַּֽעֲשֶׂ֖ה אֹתָ֑הּ שְׁלֹ֧שׁ מֵא֣וֹת אַמָּ֗ה אֹ֚רֶךְ הַתֵּבָ֔ה חֲמִשִּׁ֤ים אַמָּה֙ רָחְבָּ֔הּ וּשְׁלֹשִׁ֥ים אַמָּ֖ה קוֹמָתָֽהּ׃ "/>
                <ITEXT CPARENT="verse no" CH="טז"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="צֹ֣הַר ׀ תַּֽעֲשֶׂ֣ה לַתֵּבָ֗ה וְאֶל־אַמָּה֙ תְּכַלֶ֣נָּה מִלְמַ֔עְלָה וּפֶ֥תַח הַתֵּבָ֖ה בְּצִדָּ֣הּ תָּשִׂ֑ים תַּחְתִּיִּ֛ם שְׁנִיִּ֥ם וּשְׁלִשִׁ֖ים תַּֽעֲשֶֽׂהָ׃ "/>
                <ITEXT CPARENT="verse no" CH="יז"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וַאֲנִ֗י הִנְנִי֩ מֵבִ֨יא אֶת־הַמַּבּ֥וּל מַ֙יִם֙ עַל־הָאָ֔רֶץ לְשַׁחֵ֣ת כָּל־בָּשָׂ֗ר אֲשֶׁר־בּוֹ֙ ר֣וּחַ חַיִּ֔ים מִתַּ֖חַת הַשָּׁמָ֑יִם כֹּ֥ל אֲשֶׁר־בָּאָ֖רֶץ יִגְוָֽע׃ "/>
                <ITEXT CPARENT="verse no" CH="יח"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וַהֲקִמֹתִ֥י אֶת־בְּרִיתִ֖י אִתָּ֑ךְ וּבָאתָ֙ אֶל־הַתֵּבָ֔ה אַתָּ֕ה וּבָנֶ֛יךָ וְאִשְׁתְּךָ֥ וּנְשֵֽׁי־בָנֶ֖יךָ אִתָּֽךְ׃ "/>
                <ITEXT CPARENT="verse no" CH="יט"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וּמִכָּל־הָ֠חַי מִֽכָּל־בָּשָׂ֞ר שְׁנַ֧יִם מִכֹּ֛ל תָּבִ֥יא אֶל־הַתֵּבָ֖ה לְהַחֲיֹ֣ת אִתָּ֑ךְ זָכָ֥ר וּנְקֵבָ֖ה יִֽהְיֽוּ׃ "/>
                <ITEXT CPARENT="verse no" CH="כ"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="מֵהָע֣וֹף לְמִינֵ֗הוּ וּמִן־הַבְּהֵמָה֙ לְמִינָ֔הּ מִכֹּ֛ל רֶ֥מֶשׂ הָֽאֲדָמָ֖ה לְמִינֵ֑הוּ שְׁנַ֧יִם מִכֹּ֛ל יָבֹ֥אוּ אֵלֶ֖יךָ לְהַֽחֲיֽוֹת׃ "/>
                <ITEXT CPARENT="verse no" CH="כא"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וְאַתָּ֣ה קַח־לְךָ֗ מִכָּל־מַֽאֲכָל֙ אֲשֶׁ֣ר יֵֽאָכֵ֔ל וְאָסַפְתָּ֖ אֵלֶ֑יךָ וְהָיָ֥ה לְךָ֛ וְלָהֶ֖ם לְאָכְלָֽה׃ "/>
                <ITEXT CPARENT="verse no" CH="כב"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וַיַּ֖עַשׂ נֹ֑חַ כְּ֠כֹל אֲשֶׁ֨ר צִוָּ֥ה אֹת֛וֹ אֱלֹהִ֖ים כֵּ֥ן עָשָֽׂה׃"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="portion" CH="ס"/>
                <para LINESPMode="2" LINESP="20"/>
                <ITEXT CPARENT="reading" SCALEH="98" CH="❷"/>
                <ITEXT CPARENT="regular" SCALEH="98" CH=" "/>
                <ITEXT CPARENT="chapter no" CH="ז"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="verse no" CH="א"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וַיֹּ֤אמֶר יְהוָה֙ לְנֹ֔חַ בֹּֽא־אַתָּ֥ה וְכָל־בֵּיתְךָ֖ אֶל־הַתֵּבָ֑ה כִּֽי־אֹתְךָ֥ רָאִ֛יתִי צַדִּ֥יק לְפָנַ֖י בַּדּ֥וֹר הַזֶּֽה׃ "/>
                <ITEXT CPARENT="verse no" CH="ב"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="מִכֹּ֣ל ׀ הַבְּהֵמָ֣ה הַטְּהוֹרָ֗ה תִּֽקַּח־לְךָ֛ שִׁבְעָ֥ה שִׁבְעָ֖ה אִ֣ישׁ וְאִשְׁתּ֑וֹ וּמִן־הַבְּהֵמָ֡ה אֲ֠שֶׁר לֹ֣א טְהֹרָ֥ה הִ֛וא שְׁנַ֖יִם אִ֥ישׁ וְאִשְׁתּֽוֹ׃ "/>
                <ITEXT CPARENT="verse no" CH="ג"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="גַּ֣ם מֵע֧וֹף הַשָּׁמַ֛יִם שִׁבְעָ֥ה שִׁבְעָ֖ה זָכָ֣ר וּנְקֵבָ֑ה לְחַיּ֥וֹת זֶ֖רַע עַל־פְּנֵ֥י כָל־הָאָֽרֶץ׃ "/>
                <ITEXT CPARENT="verse no" CH="ד"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="כִּי֩ לְיָמִ֨ים ע֜וֹד שִׁבְעָ֗ה אָֽנֹכִי֙ מַמְטִ֣יר עַל־הָאָ֔רֶץ אַרְבָּעִ֣ים י֔וֹם וְאַרְבָּעִ֖ים לָ֑יְלָה וּמָחִ֗יתִי אֶֽת־כָּל־הַיְקוּם֙ אֲשֶׁ֣ר עָשִׂ֔יתִי מֵעַ֖ל פְּנֵ֥י הָֽאֲדָמָֽה׃ "/>
                <ITEXT CPARENT="verse no" CH="ה"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="regular" CH="וַיַּ֖עַשׂ נֹ֑חַ כְּכֹ֥ל אֲשֶׁר־צִוָּ֖הוּ יְהוָֽה׃ "/>
                <ITEXT CPARENT="verse no" CH="ו"/>
                <nbspace CPARENT="regular"/>
