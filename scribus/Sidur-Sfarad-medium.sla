            <StoryText>
                <DefaultStyle/>
                <ITEXT CPARENT="subtitile" CH="והוא רחום"/>
                <para/>
                <ITEXT CPARENT="G link" CH="תהלים עח"/>
                <para/>
                <ITEXT CPARENT="LC verse no" CH="לח"/>
                <ITEXT CPARENT="L regular" CH=" וְה֤וּא רַח֨וּם ׀ יְכַפֵּ֥ר עָוֺן֮, וְֽלֹא־יַ֫שְׁחִ֥ית,"/>
                <breakline/>
                <ITEXT CPARENT="L regular" CH="וְ֭הִרְבָּה לְהָשִׁ֣יב אַפּ֑וֹ, וְלֹֽא־יָ֝עִיר כָּל־חֲמָתֽוֹ׃"/>
                <para PARENT="poem"/>
                <ITEXT CPARENT="G link" CH="תהלים מ"/>
                <para PARENT="-L story"/>
                <ITEXT CPARENT="LC verse no" CH="יב"/>
                <ITEXT CPARENT="L regular" CH=" אַתָּ֤ה "/>
                <ITEXT CPARENT="L holly" CH="יְהוָ֗ה"/>
                <ITEXT CPARENT="L regular" CH=" לֹא־תִכְלָ֣א רַחֲמֶ֣יךָ מִמֶּ֑נִּי,"/>
                <breakline/>
                <ITEXT CPARENT="L regular" CH="חַסְדְּךָ֥ וַ֝אֲמִתְּךָ֗ תָּמִ֥יד יִצְּרֽוּנִי׃"/>
                <para PARENT="poem"/>
                <ITEXT CPARENT="G link" CH="תבלים קז"/>
                <para PARENT="-L story"/>
                <ITEXT CPARENT="LC verse no" CH="מז"/>
                <ITEXT CPARENT="L regular" CH=" הוֹשִׁיעֵ֨נוּ ׀ "/>
                <ITEXT CPARENT="L holly" CH="יְה֘וָ֤ה"/>
                <ITEXT CPARENT="L regular" CH=" "/>
                <ITEXT CPARENT="L holly" CH="אֱלֹהֵ֗ינוּ"/>
                <ITEXT CPARENT="L regular" CH=", וְקַבְּצֵנוּ֮ מִֽן־הַגּ֫וֹיִ֥ם,"/>
                <breakline/>
                <ITEXT CPARENT="L regular" CH="לְ֭הֹדוֹת לְשֵׁ֣ם קָדְשֶׁ֑ךָ, לְ֝הִשְׁתַּבֵּ֗חַ בִּתְהִלָּתֶֽךָ׃"/>
                <para PARENT="poem"/>
                <ITEXT CPARENT="G link" CH="תהלים קל"/>
                <para PARENT="-L story"/>
                <ITEXT CPARENT="LC verse no" CH="ג"/>
                <ITEXT CPARENT="L regular" CH=" אִם־עֲוֺנ֥וֹת תִּשְׁמָר־"/>
                <ITEXT CPARENT="L holly" CH="יָ֑הּ"/>
                <ITEXT CPARENT="L regular" CH=","/>
                <breakline/>
                <ITEXT CPARENT="L holly" CH="אֲ֝דֹנָ֗י"/>
                <ITEXT CPARENT="L regular" CH=", מִ֣י יַעֲמֹֽד׃"/>
                <para PARENT="poem"/>
                <ITEXT CPARENT="LC verse no" CH="ד"/>
                <ITEXT CPARENT="L regular" CH=" כִּֽי־עִמְּךָ֥ הַסְּלִיחָ֑ה, לְ֝מַ֗עַן תִּוָּרֵֽא׃"/>
                <para PARENT="poem"/>
                <para PARENT="-L story"/>
                <ITEXT CPARENT="L regular" CH="לֺא כַחֲטָאֵֽינוּ תַּעֲשֶׂה־לָּֽנוּ, וְלֹא כַעֲוֺנֹתֵֽינוּ תִּגְמֹל עָלֵֽינוּ:"/>
                <para PARENT="poem"/>
                <ITEXT CPARENT="G link" CH="ירמיה יד"/>
                <para PARENT="-L story"/>
                <ITEXT CPARENT="LC verse no" CH="ז (חסר)"/>
                <ITEXT CPARENT="L regular" CH=" אִם־עֲוֺנֵ֙ינוּ֙ עָ֣נוּ בָ֔נוּ, "/>
                <ITEXT CPARENT="L holly" CH="יְהוָ֕ה"/>
                <ITEXT CPARENT="L regular" CH=", עֲשֵׂ֖ה לְמַ֣עַן שְׁמֶ֑ךָ ... ׃"/>
                <para PARENT="-ּּS story"/>
                <ITEXT CPARENT="G link" CH="תהלים כה"/>
                <para PARENT="-L story"/>
                <ITEXT CPARENT="G link" CH="ו"/>
                <ITEXT CPARENT="L regular" CH=" זְכֹר־רַחֲמֶ֣יךָ "/>
                <ITEXT CPARENT="L holly" CH="יְ֭הוָה"/>
                <ITEXT CPARENT="L regular" CH=" וַחֲסָדֶ֑יךָ, כִּ֖י מֵעוֹלָ֣ם הֵֽמָּה׃"/>
                <para PARENT="-ּּS story"/>
                <ITEXT CPARENT="G link" CH="תהלים כ"/>
                <para PARENT="-L story"/>
                <ITEXT CPARENT="LC verse no" CH="ב (בשינוי)"/>
                <ITEXT CPARENT="L regular" CH=" יַֽעַנֵנוּ֣* "/>
                <ITEXT CPARENT="L holly" CH="יְ֭הוָה"/>
                <ITEXT CPARENT="L regular" CH=" בְּי֣וֹם צָרָ֑ה, יְ֝שַׂגֶּבְךָ֗ שֵׁ֤ם ׀ "/>
                <ITEXT CPARENT="L holly" CH="אֱלֹהֵ֬י"/>
                <ITEXT CPARENT="L regular" CH=" יַעֲקֹֽב׃"/>
                <para PARENT="poem"/>
                <ITEXT CPARENT="LC verse no" CH="י"/>
                <ITEXT CPARENT="L regular" CH=" "/>
                <ITEXT CPARENT="L holly" CH="יְהוָ֥ה"/>
                <ITEXT CPARENT="L regular" CH=" הוֹשִׁ֑יעָה, הַ֝מֶּ֗לֶךְ יַעֲנֵ֥נוּ בְיוֹם־קָרְאֵֽנוּ׃"/>
                <para PARENT="poem"/>
                <para PARENT="-L story"/>
                <ITEXT CPARENT="L regular" CH="אָבִֽינוּ מַלְכֵּֽנוּ, חָנֵּֽנוּ וַעֲנֵֽנוּ, כִּי אֵין בָּֽנוּ מַעֲשִׂים, עֲשֵׂה עִמָּֽנוּ צְדָקָה כְּרוֹב רַחֲמֶֽיךָ וְהוֹשִׁיעֵֽנוּ לְמַֽעַן שְׁמֶֽךָ: "/>
                <ITEXT CPARENT="L holly" CH="אֲדוֹנֵֽינוּ"/>
                <ITEXT CPARENT="L regular" CH=" "/>
                <ITEXT CPARENT="L holly" CH="אֱלֺהֵֽינוּ"/>
                <ITEXT CPARENT="L regular" CH=", שְׁמַע קוֹל תַּחֲנוּנֵֽינוּ, וּזְ֒כָר־לָֽנוּ אֶת־בְּרִית אֲבוֹתֵֽינוּ, וְהוֹשִׁיעֵֽנוּ לְמַֽעַן שְׁמֶֽךָ:"/>
                <para PARENT="-L story"/>
                <ITEXT CPARENT="G link" CH="דניאל ט"/>
                <para/>
                <ITEXT CPARENT="LC verse no" CH="ט"/>
                <ITEXT CPARENT="L regular" CH=" וְעַתָּ֣ה ׀ "/>
                <ITEXT CPARENT="L holly" CH="אֲדֹנָ֣י"/>
                <ITEXT CPARENT="L regular" CH=" "/>
                <ITEXT CPARENT="L holly" CH="אֱלֹהֵ֗ינוּ"/>
                <ITEXT CPARENT="L regular" CH=", אֲשֶׁר֩ הוֹצֵ֨אתָ אֶֽת־עַמְּךָ֜ מֵאֶ֤רֶץ מִצְרַ֙יִם֙ בְּיָ֣ד חֲזָקָ֔ה, וַתַּֽעַשׂ־לְךָ֥ שֵׁ֖ם כַּיּ֣וֹם הַזֶּ֑ה, חָטָ֖אנוּ רָשָֽׁעְנוּ׃"/>
                <ITEXT CPARENT="LC verse no" CH=" טז"/>
                <ITEXT CPARENT="L regular" CH=" "/>
                <ITEXT CPARENT="L holly" CH="אֲדֹנָ֗י"/>
                <ITEXT CPARENT="L regular" CH=", כְּכָל־צִדְקֹתֶ֙ךָ֙ יָֽשָׁב־נָ֤א אַפְּךָ֙ וַחֲמָ֣תְךָ֔ מֵעִֽירְךָ֥ יְרוּשָׁלִַ֖ם הַר־קָדְשֶׁ֑ךָ, כִּ֤י בַחֲטָאֵ֙ינוּ֙ וּבַעֲוֺנ֣וֹת אֲבֹתֵ֔ינוּ, יְרוּשָׁלִַ֧ם וְעַמְּךָ֛ לְחֶרְפָּ֖ה לְכָל־סְבִיבֹתֵֽינוּ׃"/>
                <ITEXT CPARENT="LC verse no" CH=" ז"/>
                <ITEXT CPARENT="L regular" CH=" וְעַתָּ֣ה ׀ שְׁמַ֣ע "/>
                <ITEXT CPARENT="L holly" CH="אֱלֹהֵ֗ינוּ"/>
                <ITEXT CPARENT="L regular" CH=" אֶל־תְּפִלַּ֤ת עַבְדְּךָ֙ וְאֶל־תַּ֣חֲנוּנָ֔יו, וְהָאֵ֣ר פָּנֶ֔יךָ עַל־מִקְדָּשְׁךָ֖ הַשָּׁמֵ֑ם, לְמַ֖עַן "/>
                <ITEXT CPARENT="L holly" CH="אֲדֹנָֽי"/>
                <ITEXT CPARENT="L regular" CH="׃"/>
                <ITEXT CPARENT="LC verse no" CH=" יח"/>
                <ITEXT CPARENT="L regular" CH=" הַטֵּ֨ה "/>
                <ITEXT CPARENT="L holly" CH="אֱלֹהַ֥י"/>
                <ITEXT CPARENT="L regular" CH=" ׀ אָזְנְךָ֮ וּֽשֲׁמָע֒, פְּקַ֣ח עֵינֶ֗יךָ וּרְאֵה֙ שֹֽׁמְמֹתֵ֔ינוּ, וְהָעִ֕יר אֲשֶׁר־נִקְרָ֥א שִׁמְךָ֖ עָלֶ֑יהָ, כִּ֣י ׀ לֹ֣א עַל־צִדְקֹתֵ֗ינוּ אֲנַ֨חְנוּ מַפִּילִ֤ים תַּחֲנוּנֵ֙ינוּ֙ לְפָנֶ֔יךָ, כִּ֖י עַל־רַחֲמֶ֥יךָ הָרַבִּֽים׃"/>
                <ITEXT CPARENT="LC verse no" CH=" יט"/>
                <ITEXT CPARENT="L regular" CH=" "/>
                <ITEXT CPARENT="L holly" CH="אֲדֹנָ֤י"/>
                <ITEXT CPARENT="L regular" CH=" ׀ שְׁמָ֙עָה֙, "/>
                <ITEXT CPARENT="L holly" CH="אֲדֹנָ֣י"/>
                <ITEXT CPARENT="L regular" CH=" ׀ סְלָ֔חָה, "/>
                <ITEXT CPARENT="L holly" CH="אֲדֹנָ֛י"/>
                <ITEXT CPARENT="L regular" CH=" הַֽקֲשִׁ֥יבָה, וַעֲשֵׂ֖ה אַל־תְּאַחַ֑ר, לְמַֽעֲנְךָ֣ "/>
                <ITEXT CPARENT="L holly" CH="אֱלֹהַ֔י"/>
                <ITEXT CPARENT="L regular" CH=", כִּֽי־שִׁמְךָ֣ נִקְרָ֔א עַל־עִירְךָ֖ וְעַל־עַמֶּֽךָ׃"/>
                <para PARENT="-L story"/>
                <para/>
                <ITEXT CPARENT="L regular" CH="אָבִֽינוּ אָב הָרַחֲמָן, הַרְאֵֽנוּ אוֹת לְטוֹבָה, וְקַבֵּץ נְפוּצוֹתֵֽינוּ מֵאַרְבַּע כַּנְ֒פוֹת הָאָֽר;ֶץ, יַכִּֽירוּ וְיֵדְ֒עוּ כָּל־הַגּוֹיִם כִּי אַתָּה "/>
                <ITEXT CPARENT="L holly" CH="יְהֹוָה"/>
                <ITEXT CPARENT="L regular" CH=" "/>
                <ITEXT CPARENT="L holly" CH="אֱלֺהֵֽינוּ"/>
                <ITEXT CPARENT="L regular" CH=":"/>
                <para PARENT="-L story"/>
                <ITEXT CPARENT="G link" CH="ישעיה סד"/>
                <para/>
                <ITEXT CPARENT="LC verse no" CH="ז"/>
                <ITEXT CPARENT="L regular" CH=" וְעַתָּ֥ה יְהוָ֖ה אָבִ֣ינוּ אָ֑תָּה, אֲנַ֤חְנוּ הַחֹ֙מֶר֙ וְאַתָּ֣ה יֹצְרֵ֔נוּ, וּמַעֲשֵׂ֥ה יָדְךָ֖ כֻּלָּֽנוּ׃"/>
                <para PARENT="-L story"/>
                <para/>
