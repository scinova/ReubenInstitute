                <ITEXT CPARENT="S holly" CH="יְהֹוָה"/>
                <ITEXT CPARENT="S regular" CH=" לִי וְלֹא אִירָא"/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="subtitile" CH="פיוט שלושה עשר עקרי האמונה"/>
                <para/>
                <ITEXT CPARENT="G link" CH="מיוחס לרבי דניאל בן יהודה הרומי"/>
                <para/>
                <ITEXT CPARENT="S regular" CH="יִגְדַּל "/>
                <ITEXT CPARENT="S holly" CH="אֱלֹהִים"/>
                <ITEXT CPARENT="S regular" CH=" חַי וְיִשְׁתַּבַּח, נִמְצָא וְאֵין עֵת אֶל מְצִיאוּתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="אֶחָד וְאֵין יָחִיד כְּיִחוּדוֹ, נֶעְלָם וְגַם אֵין סוֹף לְאַחְדּוּתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="אֵין לוֹ דְמוּת הַגּוּף וְאֵינוֹ גוּף, לֹא נַעֲרֹךְ אֵלָיו קְדֻשָּׁתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="קַדְמוֹן לְכָל דָּבָר אֲשֶׁר נִבְרָא, רִאשׁוֹן וְאֵין רֵאשִׁית לְרֵאשִׁיתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="הִנּוֹ אֲדוֹן עוֹלָם לְכָל נוֹצָר, יוֹרֶה גְדֻלָּתוֹ וּמַלְכוּתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="שֶׁ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="פַע נְבוּאָתוֹ נְתָנוֹ, אֶל אַנְשֵׁי סְגֻלָּתוֹ וְתִפְאַרְתּוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="לֹא קָם בְּיִשְׂרָאֵל כְּמשֶׁה עוֹד, נָבִיא וּמַבִּיט אֶת תְּמוּנָתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="תּוֹרַת אֱמֶת נָתַן לְעַמּוֹ אֵל, עַל יַד נְבִיאוֹ נֶאֱמַן בֵּיתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="לֹא יַחֲלִיף הָאֵל וְלֹא יָמִיר דָּתוֹ, לְעוֹלָמִים לְזוּלָתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="צוֹפֶה וְיוֹדֵ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="עַ סְתָרֵ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ינוּ, מַבִּיט לְסוֹף דָּבָר בְּקַדְמָתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="גּוֹמֵל לְאִישׁ חֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="סֶד כְּמִפְעָלוֹ, נוֹתֵן לְרָשָׁע רָע כְּרִשְׁעָתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="יִשְׁלַח לְקֵץ הַיָּמִין מְשִׁיחֵ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="נוּ, לִפְדּוֹת מְחַכֵּי קֵץ יְשׁוּעָתוֹ."/>
                <para PARENT="-ּS poem"/>
                <ITEXT CPARENT="S regular" CH="מֵתִים יְחַיֶּה אֵל בְּרֹב חַסְדּוֹ, בָּרוּךְ עֲדֵי עַד שֵׁם תְּהִלָּתוֹ."/>
                <trail PARENT="-ּS poem"/>
            </StoryText>
