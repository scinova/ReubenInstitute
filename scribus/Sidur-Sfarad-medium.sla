                <para/>
                <ITEXT CPARENT="S regular" CH="אֲדוֹן עוֹלָם אֲשֶׁר מָלַךְ, בְּטֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="רֶם כָּל יְצִיר נִבְרָא"/>
                <breakline/>
                <ITEXT CPARENT="S regular" CH="לְעֵת נַעֲשָׂה בְחֶפְצוֹ כֹּל, אֲזַי מֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="לֶךְ שְׁמוֹ נִקְרָא"/>
                <breakline/>
                <ITEXT CPARENT="S regular" CH="וְאַחֲרֵי כִּכְלוֹת הַכֹּל, לְבַדּוֹ יִמְלֹךְ נוֹרָא"/>
                <breakline/>
                <ITEXT CPARENT="S regular" CH="וְהוּא הָיָה וְהוּא הֹוֶה, וְהוּא יִהְיֶה בְּתִפְאָרָה"/>
                <breakline/>
                <ITEXT CPARENT="S regular" CH="וְהוּא אֶחָד וְאֵין שֵׁנִי, לְהַמְשִׁיל לוֹ לְהַחְבִּ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ירָה"/>
                <breakline/>
                <ITEXT CPARENT="S regular" CH="בְּלִי רֵאשִׁית בְּלִי תַכְלִית, וְלוֹ הָעֹז וְהַמִּשְׂרָה"/>
                <breakline/>
                <ITEXT CPARENT="S regular" CH="וְהוּא אֵלִי וְחַי גּוֹאֲלִי, וְצוּר חֶבְלִי בְּעֵת צָרָה"/>
                <breakline/>
                <ITEXT CPARENT="S regular" CH="וְהוּא נִסִּי וּמָנוֹס לִי, מְנָת כּוֹסִי בְּיוֹם אֶקְרָא"/>
                <breakline/>
                <ITEXT CPARENT="S regular" CH="בְּיָדוֹ אַפְקִיד רוּחִי, בְּעֵת אִישַׁן וְאָעִ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ירָה"/>
                <breakline/>
