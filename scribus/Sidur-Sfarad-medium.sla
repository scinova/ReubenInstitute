                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="עַן טְבוּחִים עַל יִחוּדֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
