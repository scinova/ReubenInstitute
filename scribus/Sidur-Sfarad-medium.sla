                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="דְתָּ לָהֶם בְּעֵת צָרָתָם, רַ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="בְתָּ אֶת־רִיבָם, דַּ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="נְתָּ אֶת־דִּינָם, נָקַ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="מְתָּ אֶת־נִקְמָתָם, מָסַ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="רְתָּ גִבּוֹרִים בְּיַד חַלָּשִׁים, וְרַבִּים בְּיַד מְעַטִּים, וּטְמֵאִים בְּיַד טְהוֹרִים, וּרְשָׁעִים בְּיַד צַדִּיקִים, וְזֵדִים בְּיַד עוֹסְקֵי תוֹרָתֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ךָ. וּלְךָ עָשִׂ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="יתָ שֵׁם גָּדוֹל וְקָדוֹשׁ בְּעוֹלָמֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ךָ, וּלְעַמְּךָ יִשְׂרָאֵל עָשִׂ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="יתָ תְּשׁוּעָה גְדוֹלָה וּפֻרְקָן כְּהַיּוֹם הַזֶּה. וְאַחַר־כַּךְ בָּ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="אוּ בָנֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="יךָ לִדְבִיר בֵּיתֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ךָ, וּפִנּוּ אֶת־הֵיכָלֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ךָ, וְטִהֲרוּ אֶת־מִקְדָּשֶׁ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ךָ, וְהִדְלִ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="יקוּ נֵרוֹת בְּחַצְרוֹת קָדְשֶׁ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ךָ, וְקָבְעוּ שְׁמוֹנַת יְמֵי חֲנֻכָּה אֵ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="לּוּ, לְהוֹדוֹת וּלְהַלֵּל לְשִׁמְךָ הַגָּדוֹל."/>
                <para PARENT="-ּּS story"/>
                <ITEXT CPARENT="+info" CH="בפורים:"/>
                <para ALIGN="1"/>
                <ITEXT CPARENT="S regular" CH="בִּימֵי מָרְדְּכַי וְאֶסְתֵּר בְּשׁוּשַׁן הַבִּירָה, כְּשֶׁעָמַד עֲלֵיהֶם הָמָן הָרָשָׁע, בִּקֵּשׁ לְהַשְׁמִיד לַהֲרוֹג וּלְאַבֵּד אֶת־כָּל־הַיְּהוּדִים, מִנַּ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="עַר וְעַד־זָקֵן, טַף וְנָשִׁים, בְּיוֹם אֶחָד, בִּשְׁלוֹשָׁה עָשָׂר לְחֹ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="דֶשׁ שְׁנֵים־עָשָׂר, הוּא־חֹ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="דֶשׁ אֲדָר, וּשְׁלָלָם לָבוֹז. וְאַתָּה בְּרַחֲמֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="יךָ הָרַבִּים הֵפַ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="רְתָּ אֶת־עֲצָתוֹ, וְקִלְקַ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="לְתָּ אֶת־מַחֲשַׁבְתּוֹ, וַהֲשֵׁב"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="וֹתָ לּוֹ גְּמוּלוֹ בְּרֹאשׁוֹ, וְתָלוּ אוֹתוֹ וְאֶת־בָּנָיו עַל־הָעֵץ."/>
                <para PARENT="-ּּS story"/>
                <ITEXT CPARENT="M regular" CH="וְעַל־כֻּלָּם יִתְבָּרַךְ וְיִתְרוֹמַם וְיִתְנַשֵׂא שִׁמְךָ מַלְכֵּ"/>
                <ITEXT CPARENT="M cantillation" CH="ֽ"/>
                <ITEXT CPARENT="M regular" CH="נוּ תָּמִיד לְעוֹלָם וָעֶד."/>
                <para PARENT="-M story"/>
                <ITEXT CPARENT="+info" CH="בעשרת ימי תשובה:"/>
                <para ALIGN="1"/>
                <ITEXT CPARENT="S regular" CH="וּכְתוֹב לְחַיִּים טוֹבִים כָּל בְּנֵי בְרִיתֶ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ךָ."/>
                <para PARENT="-ּּS story"/>
                <para/>
