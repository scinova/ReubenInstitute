                <ITEXT CPARENT="S regular" CH="ינוּ, "/>
                <ITEXT CPARENT="S holly" CH="אֱלֹהֵי"/>
                <ITEXT CPARENT="S regular" CH=" כָל בָּשָׂר, יוֹצְרֵ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="נוּ יוֹצֵר בְּרֵאשִׁית. בְּרָכוֹת וְהוֹדָאוֹת לְשִׁמְךָ הַגָּדוֹל וְהַקָּדוֹשׁ עַל שֶׁהֶחֱיִיתָ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="נוּ וְקִיַּמְתָּ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="נוּ. כֵּן תְּחַיֵּ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="ינוּ וּתְקַיְּמֵ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
                <ITEXT CPARENT="S regular" CH="נוּ, וְתֶאֱסוֹף גָּלֻיּוֹתֵ"/>
                <ITEXT CPARENT="S cantillation" CH="ֽ"/>
