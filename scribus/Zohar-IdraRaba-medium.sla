                <para ALIGN="3" DIRECTION="1" LINESPMode="0" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <para ALIGN="3" DIRECTION="1" LINESPMode="0" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <ITEXT CPARENT="regular" CH="- שָׁנִינוּ - אָמַר רַבִּי שִׁמְעוֹן לַחֲבֵרִים - עַד מָתַי נֵשֵׁב בְּקִיּוּם עַמּוּד אֶחָד? כָּתוּב: "/>
                <ITEXT CPARENT="citation" CH="“עֵ֭ת לַעֲשׂ֣וֹת לַיהוָ֑ה הֵ֝פֵ֗רוּ תּוֹרָתֶֽךָ”"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="link" CH="(תהלים"/>
                <nbspace CPARENT="link"/>
                <ITEXT CPARENT="link" CH="קיט"/>
                <nbspace CPARENT="link"/>
                <ITEXT CPARENT="link" CH="קכו)"/>
                <ITEXT CPARENT="regular" CH=". הַיָּמִים קְצָרִים, וּבַּעֲלֵי הַחֵטְא דּוֹחֵק. הַכָּרוֹז קוֹרֵא כָּל יוֹם, וְקוֹצְרֵי הַשָּׂדֶה מוּעָטִים הֵם, וְהֵם בְּשׁוּלֵי הַכֶּרֶם, לֹא מַשְׁגִּיחִים וְלֹא יוֹדְעִים לְאֵיזֶה מָקוֹם הוֹלְכִים כְּמוֹ שֶׁיָּאוּת."/>
                <para ALIGN="3" DIRECTION="1" LINESPMode="0" ParagraphEffectOffset="0" ParagraphEffectIndent="0" DROP="0" DROPLIN="2" Bullet="0" BulletStr="•" Numeration="0" NumerationFormat="0" NumerationName="&lt;local block&gt;" NumerationLevel="0" NumerationPrefix="" NumerationSuffix="." NumerationStart="1" NumerationHigher="1"/>
                <para PARENT="dialog"/>
                <ITEXT CPARENT="regular" CH="- הִתְאַסְּפוּ, הַחֲבֵרִים לְבֵית הָאִדְּרָא, לְבוּשִׁים שִׁרְיוֹן סַיִף, וּרְמָחִים בִּידֵיכֶם! הִזְדָּרְזוּ בְתִקּוּנֵיכֶם, בְּעֵצָה, בְּחָכְמָה, בְּשֵׂכֶל, בְּדַעַת, בְּמַרְאֶה, בַּיָּדַיִם, בָּרַגְלַיִם. הַמְלִיכוּ עֲלֵיהֶם לְמִי שֶׁבִּרְשׁוּתוֹ הַחַיִּים וְהַמָּוֶת, כְּדֵי לִגְזֹר דִּבְרֵי אֱמֶת, דְּבָרִים שֶׁקְּדוֹשִׁים עֶלְיוֹנִים מַקְשִׁיבִים לָהֶם, וּשְׂמֵחִים לִשְׁמוֹעַ אוֹתָם וְלָדַעַת אוֹתָם."/>
                <para/>
                <para/>
                <ITEXT CPARENT="regular" CH="יָשַׁב רַבִּי שִׁמְעוֹן וּבָכָה, וְאָמַר:"/>
                <para/>
                <ITEXT CPARENT="regular" CH="- אוֹי אִם אֲגַלֶּה, אוֹי אִם לֹא אֲגַלֶּה!"/>
                <para/>
                <ITEXT CPARENT="regular" CH="הַחֲבֵרִים שֶׁהָיוּ שָׁם שָׁתְקוּ. עָמַד רַבִּי אַבָּא וְאָמַר לוֹ:"/>
                <para/>
                <ITEXT CPARENT="regular" CH="- אִם נוֹחַ לִפְנֵי מַר לְגַלּוֹת, הֲרֵי כָּתוּב: "/>
                <ITEXT CPARENT="citation" CH="“סוֹד יְהֹוָה לִירֵאָיו”"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="link" CH="(תהילים"/>
                <nbspace CPARENT="link"/>
                <ITEXT CPARENT="link" CH="כה"/>
                <nbspace CPARENT="link"/>
                <ITEXT CPARENT="link" CH="יד)"/>
                <ITEXT CPARENT="regular" CH=", וַהֲרֵי חֲבֵרִים אֵלּוּ - יִרְאֵי הַקָּדוֹשׁ־בָּרוּךְ־הוּא הֵם, וּכְבָר נִכְנְסוּ בְּאִדְּרַת בֵּית־הַמִּשְׁכָּן, מֵהֶם נִכְנְסוּ, מֵהֶם יָצְאו."/>
                <para/>
                <para/>
                <ITEXT CPARENT="regular" CH="שָׁנִינוּ, שנִמְנוּ הַחֲבֵרִים לִפְנֵי רַבִּי שִׁמְעוֹן, וְנִמְצְאוּ:"/>
                <para/>
                <ITEXT CPARENT="regular" CH="רַבִּי אֶלְעָזָר בְּנוֹ,"/>
                <para PARENT="LIST" ParagraphEffectCharStyle=""/>
                <ITEXT CPARENT="regular" CH="וְרַבִּי אַבָּא,"/>
                <para PARENT="LIST" ParagraphEffectCharStyle=""/>
                <ITEXT CPARENT="regular" CH="וְרַבִּי יְהוּדָה,"/>
                <para PARENT="LIST" ParagraphEffectCharStyle=""/>
                <ITEXT CPARENT="regular" CH="וְרַבִּי יוֹסֵי בַּר־יַעֲקֹב,"/>
                <para PARENT="LIST" ParagraphEffectCharStyle=""/>
                <ITEXT CPARENT="regular" CH="וְרַבִּי יִצְחָק,"/>
                <para PARENT="LIST" ParagraphEffectCharStyle=""/>
                <ITEXT CPARENT="regular" CH="וְרַבִּי חִזְקִיָּה בַּר־רַב,"/>
                <para PARENT="LIST" ParagraphEffectCharStyle=""/>
                <ITEXT CPARENT="regular" CH="וְרַבִּי חִיָּיא,"/>
                <para PARENT="LIST" ParagraphEffectCharStyle=""/>
                <ITEXT CPARENT="regular" CH="וְרַבִּי יוֹסֵי,"/>
                <para PARENT="LIST" ParagraphEffectCharStyle=""/>
                <ITEXT CPARENT="regular" CH="וְרַבִּי יֵיסָא."/>
                <para PARENT="LIST" ParagraphEffectCharStyle=""/>
                <ITEXT CPARENT="regular" CH="יָדַיִם נָתְנוּ לְרַבִּי שִׁמְעוֹן, וְאֶצְבָּעוֹת זָקְפוּ לְמַעְלָה, וְנִכְנְסוּ בְשָׂדֶה בֵּין הָאילנות וְיָשְׁבוּ. קָם רַבִּי שִׁמְעוֹן וְהִתְפַּלֵּל תְּפִלָּתוֹ. יָשַׁב בְּתוֹכָם, וְאָמַר:"/>
                <para/>
                <ITEXT CPARENT="regular" CH="- כָּל אֶחָד יָשִׂים יָדיו בְּחָזְקוֹ."/>
                <para/>
                <ITEXT CPARENT="regular" CH="שָׂמוּ יְדֵיהֶם, וְלָקַח אוֹתָם. פָּתַח וְאָמַר:"/>
                <para/>
                <ITEXT CPARENT="regular" CH="- "/>
                <ITEXT CPARENT="citation" CH="“אָרוּר הָאִישׁ אֲשֶׁר יַעֲשֶׂה פֶסֶל וּמַסֵּכָה, תּּוֹֹעֲבַת יְהֹוָה, מַעֲשֵׂה יְדֵי חָרָשׁ, וְשָׂם בַּסָּתֶר”"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="link" CH="(דברים"/>
