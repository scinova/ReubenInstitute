                <ITEXT CPARENT="regular" CH="."/>
                <para/>
                <para/>
                <ITEXT CPARENT="regular" CH="קָרָא לְרַבִּי אֶלְעָזָר בְּנוֹ, וְהוֹשִׁיבוֹ לְפָנָיו, וּלְרַבִּי אַבָּא מהָצַּד הָאַחֵר, וְאָמַר:"/>
                <para/>
                <ITEXT CPARENT="regular" CH="- אָנוּ כְּלַל הַכֹּל. עַד עַכְשָׁו הִתְתַּקְּנוּ הָעוֹמְדִים."/>
                <para/>
                <ITEXT CPARENT="regular" CH="שָׁתְקוּ. שָׁמְעוּ קוֹל, וְאַרְכֻּבּוֹתֵיהֶם - זוֹ לְזוֹ נָקְשׁוּ. מָה הוּא הַקּוֹל? קוֹל שֶׁל כְּנֻפְיָה עֶלְיוֹנָה שֶׁל מִתְכַּנְּפִים."/>
                <para/>
                <para ALIGN="3" DIRECTION="1"/>
                <ITEXT CPARENT="regular" CH="שָׂמַח רַבִּי שִׁמְעוֹן, וְאָמַר:"/>
                <para ALIGN="3" DIRECTION="1"/>
                <ITEXT CPARENT="regular" CH="- "/>
                <ITEXT CPARENT="citation" CH="“יְהֹוָה שָׁמַעְתִּי שִׁמְעֲךְ יָרֵאתִי”"/>
                <ITEXT CPARENT="regular" CH=" "/>
                <ITEXT CPARENT="link" CH="(חבקוק ג ב)"/>
                <ITEXT CPARENT="regular" CH=". שָׁם יָאוּת הָיָה לִהְיוֹת יָרֵא. אָנוּ בַּחֲבִיבוּת תָּלוּי הַדָּבָר, מִשּׁוּם שֶׁכָּתוּב: "/>
                <ITEXT CPARENT="citation" CH="“וְאָהַבְתָּ אֵת יְהֹוָה אֱלֹהֶיךְ”"/>
                <ITEXT CPARENT="regular" CH=" "/>
                <ITEXT CPARENT="link" CH="(דברים ו ה)"/>
                <ITEXT CPARENT="regular" CH=", וְכָתוּב: "/>
                <ITEXT CPARENT="citation" CH="“מֵאַהֲבַת יְהֹוָה אֶתְכֶם”"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="link" CH="(דברים"/>
