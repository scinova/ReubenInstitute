                <ITEXT CPARENT="regular" CH="."/>
                <para/>
                <ITEXT CPARENT="regular" CH="וְעָנוּ כֻּלָּם וְאָמְרוּ אָמֵן."/>
                <para/>
                <para/>
                <ITEXT CPARENT="regular" CH="פָּתַח רַבִּי שִׁמְעוֹן וְאָמַר:"/>
                <para/>
                <ITEXT CPARENT="regular" CH="- "/>
                <ITEXT CPARENT="citation" CH="“עֵת לַעֲשׂוֹת לַיְהֹוָה”"/>
                <nbspace CPARENT="regular"/>
                <ITEXT CPARENT="link" CH="(תהלים"/>
